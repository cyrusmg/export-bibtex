@PHDTHESIS{Russell2014-od,
  title    = "Intuitive Eating, Attitudes to Food, and Body Size: A Comparison
              Between Nutrition Majors and {Non-Majors}",
  author   = "Russell, Katelyn",
  abstract = "Dietitians are expected to deliver sound and scientifically
