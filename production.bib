@ARTICLE{Koven2015-di,
  title    = "The clinical basis of orthorexia nervosa: emerging perspectives",
  author   = "Koven, Nancy S and Abry, Alexandra W",
  abstract = "Orthorexia nervosa describes a pathological obsession with proper
