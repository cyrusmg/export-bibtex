@ARTICLE{Hautmann2016-zw,
  title    = "{Cross-Sectional} Survey on Bronchoscopy in {Germany--The}
              Current Status of Clinical Practice",
  author   = "Hautmann, H and Hetzel, J and Eberhardt, R and Stanzel, F and
