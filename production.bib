}

@ARTICLE{Matsubara2017-id,
  title    = "Clinical Application of Insertion Force Sensor System for Coil
              Embolization of Intracranial Aneurysms",
  author   = "Matsubara, Noriaki and Miyachi, Shigeru and Izumi, Takashi and
              Yamada, Hiroyuki and Marui, Naoki and Ota, Keisuke and Tajima,
