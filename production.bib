@ARTICLE{Kockro2016-wb,
  title    = "Aneurysm Surgery with Preoperative {Three-Dimensional} Planning
              in a Virtual Reality Environment: Technique and Outcome Analysis",
  author   = "Kockro, Ralf A and Killeen, Tim and Ayyad, Ali and Glaser, Martin
