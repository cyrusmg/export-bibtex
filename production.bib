  journal  = "BMC Health Serv. Res.",
  volume   =  11,
  pages    = "318",
  year     =  2011,
  language = "eng"
}

@ARTICLE{Messikomer2007-tz,
  title    = "``Our options have changed. .. we will not call you back''.
              Communicating with my primary care physician",
  author   = "Messikomer, Carla M",
  abstract = "With the rise in managed care and the changes in the organization
              and delivery of health care, the medical literature is rife with
