@ARTICLE{Welch2007-cr,
  title    = "Mapping the 24-Hour Emergency Department Cycle to Improve Patient
              Flow",
  author   = "Welch, Shari J and Jones, Spencer S and Allen, Todd",
  abstract = "Background:Intermountain Healthcare (Salt Lake City), in
