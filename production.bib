@ARTICLE{Miller2009-cz,
  title     = "Toward a theory of motivational interviewing",
  author    = "Miller, William R and Rose, Gary S",
  abstract  = "The widely disseminated clinical method of motivational
               interviewing (MI) arose through a convergence of science and
