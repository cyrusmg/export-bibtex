@ARTICLE{McCahon2015-so,
  title    = "Cost comparison of re-usable and single-use fibrescopes in a
              large English teaching hospital",
  author   = "McCahon, R A and Whynes, D K",
  abstract = "A number of studies in the USA and mainland Europe have described
              the costs of fibreoptic tracheal intubation. However, no such
              data from the UK appear available. We performed a cost assessment
              of fibreoptic intubation, using re-usable (various devices from
              Olympus, Acutronic and Karl Storz) and single-use
