@ARTICLE{Khandhar2017-nb,
  title    = "Electromagnetic navigation bronchoscopy to access lung lesions in
              1,000 subjects: first results of the prospective, multicenter
              {NAVIGATE} study",
  author   = "Khandhar, Sandeep J and Bowling, Mark R and Flandes, Javier and
